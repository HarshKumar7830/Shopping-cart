 return cart;\r\n};\r\n\r\nexport const ShopContextProvider = (props) => {\r\n  const [cartItems, setCartItems] = useState(getDefaultCart());\r\n\r\n  const getTotalCartAmount = () => {\r\n    let totalAmount = 0;\r\n    for (const item in cartItems) {\r\n      if (cartItems[item] > 0) {\r\n        let itemInfo = PRODUCTS.find((product) => product.id === Number(item));\r\n        totalAmount += cartItems[item] * itemInfo.price;\r\n      }\r\n    }\r\n    return totalAmount;\r\n  };\r\n\r\n  const addToCart = (itemId) => {\r\n    setCartItems((prev) => ({ ...prev, [itemId]: prev[itemId] + 1 }));\r\n  };\r\n\r\n  const removeFromCart = (itemId) => {\r\n    setCartItems((prev) => ({ ...prev, [itemId]: prev[itemId] - 1 }));\r\n  };\r\n\r\n  const updateCartItemCount = (newAmount, itemId) => {\r\n    setCartItems((prev) => ({ ...prev, [itemId]: newAmount }));\r\n  };\r\n\r\n  const checkout = () => {\r\n    setCartItems(getDefaultCart());\r\n  };\r\n\r\n  const contextValue = {\r\n    cartItems,\r\n    addToCart,\r\n    updateCartItemCount,\r\n    removeFromCart,\r\n    getTotalCartAmount,\r\n    checkout,\r\n  };\r\n\r\n  return (\r\n    <ShopContext.Provider value={contextValue}>\r\n      {props.children}\r\n    </ShopContext.Provider>\r\n  );\r\n};\r\n","C:\\Users\\AVITA\\Desktop\\Harsh-React\\Shopping-Cart\\shopping-cart-react\\src\\pages\\shop\\shop.jsx",[],[],"C:\\Users\\AVITA\\Desktop\\Harsh-React\\Shopping-Cart\\shopping-cart-react\\src\\products.js",[],[],"C:\\Users\\AVITA\\Desktop\\Harsh-React\\Shopping-Cart\\shopping-cart-react\\src\\pages\\cart\\cart-item.jsx",["59"],[],"import React, { useContext } from \"react\";\r\nimport { ShopContext } from \"../../context/shop-context\";\r\n\r\nexport const CartItem = (props) => {\r\n  const { id, productName, price, productImage } = props.data;\r\n  const { cartItems, addToCart, removeFromCart, updateCartItemCount } =\r\n    useContext(ShopContext);\r\n\r\n  return (\r\n    <div className=\"cartItem\">\r\n      <img src={productImage} />\r\n      <div className=\"description\">\r\n        <p>\r\n          <b>{productName}</b>\r\n        </p>\r\n        <p> Price: ${price}</p>\r\n        <div className=\"countHandler\">\r\n          <button onClick={() => removeFromCart(id)}> - </button>\r\n          <input\r\n            value={cartItems[id]}\r\n            onChange={(e) => updateCartItemCount(Number(e.target.value), id)}\r\n          />\r\n          <button onClick={() => addToCart(id)}> + </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n","C:\\Users\\AVITA\\Desktop\\Harsh-React\\Shopping-Cart\\shopping-cart-react\\src\\pages\\shop\\product.jsx",["60"],[],"import React, { useContext } from \"react\";\r\nimport { ShopContext } from \"../../context/shop-context\";\r\n\r\nexport const Product = (props) => {\r\n  const { id, productName, price, productImage } = props.data;\r\n  const { addToCart, cartItems } = useContext(ShopContext);\r\n\r\n  const cartItemCount = cartItems[id];\r\n\r\n  return (\r\n    <div className=\"product\">\r\n      <img src={productImage} />\r\n      <div className=\"description\">\r\n        <p>\r\n          <b>{productName}</b>\r\n        </p>\r\n        <p> ${price}</p>\r\n      </div>\r\n      <button className=\"addToCartBttn\" onClick={() => addToCart(id)}>\r\n        Add To Cart {cartItemCount > 0 && <> ({cartItemCount})</>}\r\n      </button>\r\n    </div>\r\n  );\r\n};\r\n",{"ruleId":"61","severity":1,"message":"62","line":20,"column":33,"nodeType":"63","messageId":"64","endLine":20,"endColumn":35},{"ruleId":"65","severity":1,"message":"66","line":1,"column":25,"nodeType":"67","messageId":"68","endLine":1,"endColumn":34},{"ruleId":"69","severity":1,"message":"70","line":11,"column":7,"nodeType":"71","endLine":11,"endColumn":33},{"ruleId":"69","severity":1,"message":"70","line":12,"column":7,"nodeType":"71","endLine":12,"endColumn":33},"array-callback-return","Array.prototype.map() expects a value to be returned at the end 